--- 
layout: null 
---

<?xml version="1.0" encoding="utf-8"?>
    <feed xmlns="http://www.w3.org/2005/Atom">
        <id>http://rafaeltavares.tk/blog</id>
        <title>Blog - Rafael Tavares</title>
        <icon>http://rafaeltavares.tk/public/favicon.ico</icon>
        <link href="http://rafaeltavares.tk/old/feed/index.xml" rel="self" />
        <link href="http://rafaeltavares.tk/old/blog" /> {% for post in site.posts limit:10 %} {% if post.layout == 'post' %}
        <entry>
            <id>http://rafaeltavares.tk{{ post.id }}</id>
            <title>{{ post.title | strip_html }}</title>
            <updated>{{ post.date | date_to_xmlschema }}</updated>
            <link href="http://rafaeltavares.tk/{{ post.url }}" />
            <author>
                <name>Rafael Tavares</name>
                <uri>http://rafaeltavares.tk/</uri>
            </author>
            <content type="html">
                {{ post.content | replace: "src='/old/assets/", "src='http://rafaeltavares.tk/old/assets/" | xml_escape }}
            </content>
        </entry>
        {% endif %} {% endfor %}
    </feed>